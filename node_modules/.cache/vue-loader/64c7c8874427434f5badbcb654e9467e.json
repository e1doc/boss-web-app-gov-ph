n_approver') || groups.includes('building_application_approver')\">\n                    <router-link :to=\"{name:'Inquiries'}\">\n                        <font-awesome-icon icon=\"comment-alt\" class=\"admin-icon\"/>\n                        INQUIRIES \n                    </router-link>\n                </li>\n                <li class=\"admin-item\" :class=\"{ active: currentAdminTab  === 'departments'}\" @click=\"changeTab('departments')\" v-if=\"groups.includes('superadmin')\">\n                    <router-link :to=\"{name:'Departments'}\">\n                        <font-awesome-icon icon=\"network-wired\" class=\"admin-icon\"/>\n                        DEPARTMENTS \n                    </router-link>\n                </li>\n            </ul>\n        </div>\n    </div>\n</template>\n\n<script>\nimport {mapGetters} from \"vuex\"\nexport default {\n    name : \"SidebarNav\",\n    data(){\n        return{\n            activeTab: \"dashboard\",\n        }\n    },\n  computed:{\n      ...mapGetters(['currentAdminTab', \"groups\"])\n  },\n  mounted(){\n      console.log('groups',this.groups)\n  },\n  methods:{\n    changeTab(tab){\n      this.activeTab = tab\n      this.$store.commit('setAdminCurrentTab', tab)\n    },\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\ndiv.admin-sidebar{\n    height: 100vh;\n    width: 290px;\n    background-color: #EAF6FF;\n    box-shadow: 0px 10px 8px #00000012;\n    position : fixed;\n    left : 0;\n    top : 0;\n    overflow-y: scroll;\n    div.admin-container{\n        width : 100%;\n        display : flex;\n        flex-wrap : wrap;\n        text-align : center;\n        padding : 60px 0;\n        div.admin-img{\n            width : 100%;\n            margin-bottom : 20px;\n            img{\n                width : 60px;\n                margin : 0 auto; \n            }\n        }\n        div.admin-text{\n            width : 100%;\n            font-size : 16px;\n            font-weight : bold;\n            line-height : 29px;